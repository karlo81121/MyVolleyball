<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myvolleyball">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyVolleyball">
        <activity
            android:name=".teamInfoActivity"
            android:exported="false" />
        <activity
            android:name=".playerInfoActivity"
            android:exported="false" />
        <activity
            android:name=".allResultsActivity"
            android:exported="false" />
        <activity
            android:name=".allTeamsActivity"
            android:exported="false" />
        <activity
            android:name=".allPlayersActivity"
            android:exported="false" />
        <activity
            android:name=".allGamesActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>